{
  "expo": {
    "name": "Apricot & Nut",
    "slug": "customer-mobile-app",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/apricot-nut-logo-word.png",
    "splash": {
      "image": "./assets/images/apricot-nut-logo-word.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.rt04capstone.apricotcustomer",
      "permissions": ["CAMERA"],
      "googleServicesFile": "./google-services.json"
    }
  }
}
